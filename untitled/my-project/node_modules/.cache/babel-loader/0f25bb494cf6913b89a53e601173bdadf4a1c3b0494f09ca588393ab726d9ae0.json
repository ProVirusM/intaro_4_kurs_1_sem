{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nimport ProfessionInput from './components/ProfessionInput.vue';\nimport CityInput from './components/CityInput.vue';\nimport PhotoInput from './components/PhotoInput.vue';\nimport NameInput from './components/NameInput.vue';\nimport EmailInput from './components/EmailInput.vue';\nimport PhoneInput from './components/PhoneInput.vue';\nimport BirthdayInput from './components/BirthdayInput.vue';\nimport EducationInput from './components/EducationInput.vue';\nimport SalaryInput from './components/SalaryInput.vue';\nimport SkillsInput from './components/SkillsInput.vue';\nimport AboutMeInput from './components/AboutMeInput.vue';\nexport default {\n  components: {\n    ProfessionInput,\n    CityInput,\n    PhotoInput,\n    NameInput,\n    EmailInput,\n    PhoneInput,\n    BirthdayInput,\n    EducationInput,\n    SalaryInput,\n    SkillsInput,\n    AboutMeInput\n  },\n  setup() {\n    const profession = ref('');\n    const city = ref('');\n    const photo = ref('');\n    const name = ref('');\n    const email = ref('');\n    const phone = ref('');\n    const birthday = ref('');\n    const status = ref('new');\n\n    // Инициализация educationInfo с пустыми значениями для предотвращения ошибок.\n    const educationInfo = ref([{\n      education: '',\n      institution: '',\n      faculty: '',\n      specialization: '',\n      yearOfGraduation: ''\n    }]);\n    const salary = ref('');\n    const skills = ref('');\n    const about_me = ref('');\n    const educationVars = ref([{\n      name: 'Среднее',\n      value: 1\n    }, {\n      name: 'Среднее специальное',\n      value: 2\n    }, {\n      name: 'Неоконченное высшее',\n      value: 3\n    }, {\n      name: 'Высшее',\n      value: 4\n    }]);\n    const addEducation = () => {\n      educationInfo.value.push({\n        education: '',\n        institution: '',\n        faculty: '',\n        specialization: '',\n        yearOfGraduation: ''\n      });\n    };\n    const removeEducation = index => {\n      educationInfo.value.splice(index, 1);\n    };\n    return {\n      educationInfo,\n      profession,\n      city,\n      photo,\n      name,\n      email,\n      phone,\n      birthday,\n      salary,\n      skills,\n      about_me,\n      educationVars,\n      status,\n      addEducation,\n      removeEducation\n    };\n  }\n};","map":{"version":3,"names":["ref","ProfessionInput","CityInput","PhotoInput","NameInput","EmailInput","PhoneInput","BirthdayInput","EducationInput","SalaryInput","SkillsInput","AboutMeInput","components","setup","profession","city","photo","name","email","phone","birthday","status","educationInfo","education","institution","faculty","specialization","yearOfGraduation","salary","skills","about_me","educationVars","value","addEducation","push","removeEducation","index","splice"],"sources":["D:\\Users\\danil\\untitled\\my-project\\src\\App.vue"],"sourcesContent":["<!-- App.vue -->\n<template>\n  <div class=\"container\">\n    <div class=\"form-container\">\n      <h2>Форма резюме</h2>\n      <ProfessionInput v-model=\"profession\" />\n      <CityInput v-model=\"city\" />\n      <PhotoInput v-model=\"photo\" />\n      <NameInput v-model=\"name\" />\n      <EmailInput v-model=\"email\" />\n      <PhoneInput v-model=\"phone\" />\n      <BirthdayInput v-model=\"birthday\" />\n      <p>Статус:</p>\n      <select v-model=\"status\">\n        <option value=\"new\">Новый</option>\n        <option value=\"interview\">Назначено собеседование</option>\n        <option value=\"hired\">Принят</option>\n        <option value=\"rejected\">Отказ</option>\n      </select>\n\n      <div v-for=\"(education, index) in educationInfo\" :key=\"index\" class=\"education-block\">\n        <EducationInput v-model=\"education\" :educationVars=\"educationVars\" />\n        <button @click=\"removeEducation(index)\" class=\"remove-education-btn\">×</button>\n      </div>\n      <button @click=\"addEducation\" class=\"add-education-btn\">Указать еще одно место обучения</button>\n\n      <SalaryInput v-model=\"salary\" />\n      <SkillsInput v-model=\"skills\" />\n      <AboutMeInput v-model=\"about_me\" />\n    </div>\n\n    <div class=\"summary-container\">\n      <h2>Резюме</h2>\n      <p>Профессия: <span>{{ profession }}</span></p>\n      <p>Город: <span>{{ city }}</span></p>\n      <p>Фото:</p>\n      <div class=\"image-container\">\n        <img v-if=\"photo\" :src=\"photo\" width=\"200\" height=\"100\" alt=\"Фото\" class=\"profile-photo\" />\n      </div>\n      <p>Имя: <span>{{ name }}</span></p>\n      <p>Email: <span>{{ email }}</span></p>\n      <p>Телефон: <span>{{ phone }}</span></p>\n      <p>День Рождения: <span>{{ birthday }}</span></p>\n\n      <p>Образования:</p>\n      <div v-for=\"(education, index) in educationInfo\" :key=\"index\">\n        <div v-show=\"education.education?.name !== 'Среднее' && education.education?.name !== undefined\">\n          <p>Учебное заведение: <span>{{ education.institution }}</span></p>\n          <p>Факультет: <span>{{ education.faculty }}</span></p>\n          <p>Специализация: <span>{{ education.specialization }}</span></p>\n          <p>Год окончания: <span>{{ education.yearOfGraduation }}</span></p>\n        </div>\n      </div>\n      <p>Желаемая зарплата: <span>{{ salary }}</span></p>\n      <p>Ключевые навыки: <span>{{ skills }}</span></p>\n      <p>О себе: <span>{{ about_me }}</span></p>\n      <p>Статус: <span>{{ status }}</span></p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport ProfessionInput from './components/ProfessionInput.vue'\nimport CityInput from './components/CityInput.vue'\nimport PhotoInput from './components/PhotoInput.vue'\nimport NameInput from './components/NameInput.vue'\nimport EmailInput from './components/EmailInput.vue'\nimport PhoneInput from './components/PhoneInput.vue'\nimport BirthdayInput from './components/BirthdayInput.vue'\nimport EducationInput from './components/EducationInput.vue'\nimport SalaryInput from './components/SalaryInput.vue'\nimport SkillsInput from './components/SkillsInput.vue'\nimport AboutMeInput from './components/AboutMeInput.vue'\n\nexport default {\n  components: {\n    ProfessionInput,\n    CityInput,\n    PhotoInput,\n    NameInput,\n    EmailInput,\n    PhoneInput,\n    BirthdayInput,\n    EducationInput,\n    SalaryInput,\n    SkillsInput,\n    AboutMeInput,\n  },\n\n  setup() {\n    const profession = ref('')\n    const city = ref('')\n    const photo = ref('')\n    const name = ref('')\n    const email = ref('')\n    const phone = ref('')\n    const birthday = ref('')\n    const status = ref('new')\n\n    // Инициализация educationInfo с пустыми значениями для предотвращения ошибок.\n    const educationInfo = ref([{\n      education: '',\n      institution: '',\n      faculty: '',\n      specialization: '',\n      yearOfGraduation: '',\n    }]);\n\n    const salary = ref('')\n    const skills = ref('')\n    const about_me = ref('')\n\n    const educationVars = ref([\n      { name: 'Среднее', value: 1 },\n      { name: 'Среднее специальное', value: 2 },\n      { name: 'Неоконченное высшее', value: 3 },\n      { name: 'Высшее', value: 4 }\n    ])\n\n    const addEducation = () => {\n      educationInfo.value.push({\n        education: '',\n        institution: '',\n        faculty: '',\n        specialization: '',\n        yearOfGraduation: '',\n      });\n    }\n\n    const removeEducation = (index) => {\n      educationInfo.value.splice(index, 1);\n    }\n\n    return { educationInfo, profession, city, photo, name, email, phone, birthday, salary, skills, about_me, educationVars, status, addEducation, removeEducation }\n  }\n}\n</script>\n\n\n<style>\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  font-family: Arial, sans-serif;\n  background-color: #f0f4f8;\n  color: #333;\n  display: flex;\n  justify-content: center;\n  padding-top: 50px;\n}\n\n.container {\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  gap: 20px;\n  width: 700px; /* Фиксированная ширина контейнера */\n}\n\n.form-container, .summary-container {\n  background-color: #fff;\n  padding: 20px;\n  border-radius: 8px;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n  width: 320px; /* Фиксированная ширина для каждого блока */\n  min-height: 250px; /* Минимальная высота для симметрии */\n}\n\nh2 {\n  font-size: 1.5em;\n  color: #007acc;\n  margin-bottom: 20px;\n}\n\n.form-container > * {\n  margin-bottom: 15px;\n}\n\ninput {\n  width: 100%;\n  padding: 10px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  font-size: 1em;\n}\n\ninput:focus {\n  border-color: #007acc;\n  outline: none;\n}\n\n.summary-container > p {\n  display: flex;\n  align-items: center;\n  font-size: 1em;\n  padding: 5px 0;\n}\n\n.summary-container > p span {\n  margin-left: 10px;\n  font-weight: bold;\n  color: #333;\n}\n</style>\n"],"mappings":";AA8DA,SAASA,GAAE,QAAS,KAAI;AACxB,OAAOC,eAAc,MAAO,kCAAiC;AAC7D,OAAOC,SAAQ,MAAO,4BAA2B;AACjD,OAAOC,UAAS,MAAO,6BAA4B;AACnD,OAAOC,SAAQ,MAAO,4BAA2B;AACjD,OAAOC,UAAS,MAAO,6BAA4B;AACnD,OAAOC,UAAS,MAAO,6BAA4B;AACnD,OAAOC,aAAY,MAAO,gCAA+B;AACzD,OAAOC,cAAa,MAAO,iCAAgC;AAC3D,OAAOC,WAAU,MAAO,8BAA6B;AACrD,OAAOC,WAAU,MAAO,8BAA6B;AACrD,OAAOC,YAAW,MAAO,+BAA8B;AAEvD,eAAe;EACbC,UAAU,EAAE;IACVX,eAAe;IACfC,SAAS;IACTC,UAAU;IACVC,SAAS;IACTC,UAAU;IACVC,UAAU;IACVC,aAAa;IACbC,cAAc;IACdC,WAAW;IACXC,WAAW;IACXC;EACF,CAAC;EAEDE,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAId,GAAG,CAAC,EAAE;IACzB,MAAMe,IAAG,GAAIf,GAAG,CAAC,EAAE;IACnB,MAAMgB,KAAI,GAAIhB,GAAG,CAAC,EAAE;IACpB,MAAMiB,IAAG,GAAIjB,GAAG,CAAC,EAAE;IACnB,MAAMkB,KAAI,GAAIlB,GAAG,CAAC,EAAE;IACpB,MAAMmB,KAAI,GAAInB,GAAG,CAAC,EAAE;IACpB,MAAMoB,QAAO,GAAIpB,GAAG,CAAC,EAAE;IACvB,MAAMqB,MAAK,GAAIrB,GAAG,CAAC,KAAK;;IAExB;IACA,MAAMsB,aAAY,GAAItB,GAAG,CAAC,CAAC;MACzBuB,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,EAAE;MACXC,cAAc,EAAE,EAAE;MAClBC,gBAAgB,EAAE;IACpB,CAAC,CAAC,CA