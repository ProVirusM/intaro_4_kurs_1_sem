template>\r\n  <div>\r\n    <label for=\"city\">Город:</label>\r\n    <input\r\n        id=\"city\"\r\n        type=\"text\"\r\n        v-model=\"query\"\r\n        @input=\"fetchCities\"\r\n        placeholder=\"Начните вводить название города\"\r\n    />\r\n    <ul v-if=\"cities.length\" class=\"suggestions\">\r\n      <li\r\n          v-for=\"city in cities\"\r\n          :key=\"city.id\"\r\n          @click=\"selectCity(city)\"\r\n      >\r\n        {{ city.title }}\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: {\r\n    modelValue: String, // Значение, переданное из родительского компонента\r\n  },\r\n  emits: ['update:modelValue'], // Разрешаем emit для v-model\r\n  data() {\r\n    return {\r\n      query: this.modelValue || '', // Локальное состояние для поля ввода\r\n      cities: [],\r\n    };\r\n  },\r\n  watch: {\r\n    // Синхронизируем изменения из родительского компонента\r\n    modelValue(newValue) {\r\n      if (newValue !== this.query) {\r\n        this.query = newValue;\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchCities() {\r\n      if (this.query.length < 2) {\r\n        this.cities = [];\r\n        return;\r\n      }\r\n      try {\r\n        const response = await axios.get('/api/database.getCities', {\r\n          params: {\r\n            access_token: process.env.VUE_APP_VK_ACCESS_TOKEN,\r\n            v: process.env.VUE_APP_VK_API_VERSION,\r\n            q: this.query,\r\n            country_id: 1,\r\n            count: 10,\r\n          },\r\n        });\r\n        this.cities = response.data.response.items;\r\n      } catch (error) {\r\n        console.error('Ошибка при получении городов:', error);\r\n      }\r\n    },\r\n    selectCity(city) {\r\n      this.query = city.title; // Локально обновляем поле ввода\r\n      this.cities = []; // Очищаем список подсказок\r\n      this.$emit('update:modelValue', city.title); // Сообщаем родителю о выбранном значении\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.suggestions {\r\n  list-style-type: none;\r\n  margin: 0;\r\n  padding: 0;\r\n  border: 1px solid #ddd;\r\n  max-height: 150px;\r\n  overflow-y: auto;\r\n  background: #fff;\r\n}\r\n\r\n.suggestions li {\r\n  padding: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.suggestions li:hover {\r\n  background: #f0f0f0;\r\n}\r\n</style>\r\n"],"mappings":";;EAAAA,GAAA;EAU6BC,KAAK,EAAC;;mBAVnC;;uBACEC,mBAAA,CAkBM,c,0BAjBJC,mBAAA,CAAgC;IAAzBC,GAAG,EAAC;EAAM,GAAC,QAAM,sB,gBACxBD,mBAAA,CAME;IALEE,EAAE,EAAC,MAAM;IACTC,IAAI,EAAC,MAAM;IALnB,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAMiBC,KAAA,CAAAC,KAAK,GAAAF,MAAA;IACbG,OAAK,EAAAJ,MAAA,QAAAA,MAAA,UAAAK,IAAA,KAAEC,QAAA,CAAAC,WAAA,IAAAD,QAAA,CAAAC,WAAA,IAAAF,IAAA,CAAW;IACnBG,WAAW,EAAC;iEAFHN,KAAA,CAAAC,KAAK,E,GAIRD,KAAA,CAAAO,MAAM,CAACC,MAAM,I,cAAvBf,mBAAA,CAQK,MARLgB,UAQK,I,kBAPHhB,mBAAA,CAMKiB,SAAA,QAjBXC,WAAA,CAYyBX,KAAA,CAAAO,MAAM,EAAdK,IAAI;yBADfnB,mBAAA,CAMK;MAJAF,GAAG,EAAEqB,IAAI,CAAChB,EAAE;MACZiB,OAAK,EAAAd,MAAA,IAAEK,QAAA,CAAAU,UAAU,CAACF,IAAI;wBAEtBA,IAAI,CAACG,KAAK,wBAhBrBC,UAAA;sCAAAC,mBAAA,e","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}