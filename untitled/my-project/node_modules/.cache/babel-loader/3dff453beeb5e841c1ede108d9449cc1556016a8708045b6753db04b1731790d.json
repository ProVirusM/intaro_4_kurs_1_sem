{"ast":null,"code":"import axios from 'axios';\nexport default {\n  props: {\n    modelValue: String // Значение, переданное из родительского компонента\n  },\n  emits: ['update:modelValue'],\n  // Разрешаем emit для v-model\n  data() {\n    return {\n      query: this.modelValue || '',\n      // Локальное состояние для поля ввода\n      cities: []\n    };\n  },\n  watch: {\n    // Синхронизируем изменения из родительского компонента\n    modelValue(newValue) {\n      if (newValue !== this.query) {\n        this.query = newValue;\n      }\n    }\n  },\n  methods: {\n    async fetchCities() {\n      if (this.query.length < 2) {\n        this.cities = [];\n        return;\n      }\n      try {\n        const response = await axios.get('/api/database.getCities', {\n          params: {\n            access_token: process.env.VUE_APP_VK_ACCESS_TOKEN,\n            v: process.env.VUE_APP_VK_API_VERSION,\n            q: this.query,\n            country_id: 1,\n            count: 10\n          }\n        });\n        this.cities = response.data.response.items;\n      } catch (error) {\n        console.error('Ошибка при получении городов:', error);\n      }\n    },\n    selectCity(city) {\n      this.query = city.title; // Локально обновляем поле ввода\n      this.cities = []; // Очищаем список подсказок\n      this.$emit('update:modelValue', city.title); // Сообщаем родителю о выбранном значении\n    }\n  }\n};","map":{"version":3,"names":["axios","props","modelValue","String","emits","data","query","cities","watch","newValue","methods","fetchCities","length","response","get","params","access_token","process","env","VUE_APP_VK_ACCESS_TOKEN","v","VUE_APP_VK_API_VERSION","q","country_id","count","items","error","console","selectCity","city","title","$emit"],"sources":["D:\\Users\\danil\\untitled\\my-project\\src\\components\\CityInput.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <label for=\"city\">Город:</label>\r\n    <input\r\n        id=\"city\"\r\n        type=\"text\"\r\n        v-model=\"query\"\r\n        @input=\"fetchCities\"\r\n        placeholder=\"Начните вводить название города\"\r\n    />\r\n    <ul v-if=\"cities.length\" class=\"suggestions\">\r\n      <li\r\n          v-for=\"city in cities\"\r\n          :key=\"city.id\"\r\n          @click=\"selectCity(city)\"\r\n      >\r\n        {{ city.title }}\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: {\r\n    modelValue: String, // Значение, переданное из родительского компонента\r\n  },\r\n  emits: ['update:modelValue'], // Разрешаем emit для v-model\r\n  data() {\r\n    return {\r\n      query: this.modelValue || '', // Локальное состояние для поля ввода\r\n      cities: [],\r\n    };\r\n  },\r\n  watch: {\r\n    // Синхронизируем изменения из родительского компонента\r\n    modelValue(newValue) {\r\n      if (newValue !== this.query) {\r\n        this.query = newValue;\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchCities() {\r\n      if (this.query.length < 2) {\r\n        this.cities = [];\r\n        return;\r\n      }\r\n      try {\r\n        const response = await axios.get('/api/database.getCities', {\r\n          params: {\r\n            access_token: process.env.VUE_APP_VK_ACCESS_TOKEN,\r\n            v: process.env.VUE_APP_VK_API_VERSION,\r\n            q: this.query,\r\n            country_id: 1,\r\n            count: 10,\r\n          },\r\n        });\r\n        this.cities = response.data.response.items;\r\n      } catch (error) {\r\n        console.error('Ошибка при получении городов:', error);\r\n      }\r\n    },\r\n    selectCity(city) {\r\n      this.query = city.title; // Локально обновляем поле ввода\r\n      this.cities = []; // Очищаем список подсказок\r\n      this.$emit('update:modelValue', city.title); // Сообщаем родителю о выбранном значении\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.suggestions {\r\n  list-style-type: none;\r\n  margin: 0;\r\n  padding: 0;\r\n  border: 1px solid #ddd;\r\n  max-height: 150px;\r\n  overflow-y: auto;\r\n  background: #fff;\r\n}\r\n\r\n.suggestions li {\r\n  padding: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.suggestions li:hover {\r\n  background: #f0f0f0;\r\n}\r\n</style>\r\n"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE;IACLC,UAAU,EAAEC,MAAM,CAAE;EACtB,CAAC;EACDC,KAAK,EAAE,CAAC,mBAAmB,CAAC;EAAE;EAC9BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,IAAI,CAACJ,UAAS,IAAK,EAAE;MAAE;MAC9BK,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACL;IACAN,UAAUA,CAACO,QAAQ,EAAE;MACnB,IAAIA,QAAO,KAAM,IAAI,CAACH,KAAK,EAAE;QAC3B,IAAI,CAACA,KAAI,GAAIG,QAAQ;MACvB;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,IAAI,CAACL,KAAK,CAACM,MAAK,GAAI,CAAC,EAAE;QACzB,IAAI,CAACL,MAAK,GAAI,EAAE;QAChB;MACF;MACA,IAAI;QACF,MAAMM,QAAO,GAAI,MAAMb,KAAK,CAACc,GAAG,CAAC,yBAAyB,EAAE;UAC1DC,MAAM,EAAE;YACNC,YAAY,EAAEC,OAAO,CAACC,GAAG,CAACC,uBAAuB;YACjDC,CAAC,EAAEH,OAAO,CAACC,GAAG,CAACG,sBAAsB;YACrCC,CAAC,EAAE,IAAI,CAAChB,KAAK;YACbiB,UAAU,EAAE,CAAC;YACbC,KAAK,EAAE;UACT;QACF,CAAC,CAAC;QACF,IAAI,CAACjB,MAAK,GAAIM,QAAQ,CAACR,IAAI,CAACQ,QAAQ,CAACY,KAAK;MAC5C,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF,CAAC;IACDE,UAAUA,CAACC,IAAI,EAAE;MACf,IAAI,CAACvB,KAAI,GAAIuB,IAAI,CAACC,KAAK,EAAE;MACzB,IAAI,CAACvB,MAAK,GAAI,EAAE,EAAE;MAClB,IAAI,CAACwB,KAAK,CAAC,mBAAmB,EAAEF,IAAI,CAACC,KAAK,CAAC,EAAE;IAC/C;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}