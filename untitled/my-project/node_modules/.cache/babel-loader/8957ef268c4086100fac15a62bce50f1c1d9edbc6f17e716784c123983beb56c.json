{"ast":null,"code":"import { ref, watch, computed } from 'vue';\nexport default {\n  props: {\n    modelValue: String,\n    educationVars: {\n      type: Array,\n      default() {\n        return [];\n      }\n    }\n  },\n  setup(props, {\n    emit\n  }) {\n    const SelectedEducationVars = ref(null);\n    const educationValue = ref(props.modelValue);\n    const toggleSelectedEducationVars = educationVar => {\n      SelectedEducationVars.value = educationVar;\n      emit('update:modelValue', educationValue);\n    };\n    const mappedSelectedEducationVars = computed(() => {\n      return SelectedEducationVars.value?.name || SelectedEducationVars.value || 'Выбери';\n    });\n    watch(() => props.modelValue, newVal => {\n      educationValue.value = newVal;\n    });\n    return {\n      educationValue,\n      SelectedEducationVars,\n      toggleSelectedEducationVars,\n      mappedSelectedEducationVars\n    };\n  }\n};","map":{"version":3,"names":["ref","watch","computed","props","modelValue","String","educationVars","type","Array","default","setup","emit","SelectedEducationVars","educationValue","toggleSelectedEducationVars","educationVar","value","mappedSelectedEducationVars","name","newVal"],"sources":["D:\\Users\\danil\\untitled\\my-project\\src\\components\\EducationInput.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <label>Образование:</label>\r\n    <input type=\"text\" v-model=\"educationValue\" @input=\"$emit('update:modelValue', educationValue)\" />\r\n    <div>\r\n      {{mappedSelectedEducationVars}}\r\n    </div>\r\n    <p v-for=\"(educationVar, index) in educationVars\" :key=\"index\" @click=\"toggleSelectedEducationVars(educationVar)\">\r\n      {{educationVar.name || educationVar}}\r\n    </p>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, watch, computed} from 'vue'\r\n\r\nexport default {\r\n  props: {\r\n    modelValue: String,\r\n    educationVars: {\r\n      type: Array,\r\n      default(){\r\n        return []\r\n      }\r\n    }\r\n  },\r\n  setup(props, {emit}) {\r\n    const SelectedEducationVars=ref(null)\r\n    const educationValue = ref(props.modelValue)\r\n\r\n    const toggleSelectedEducationVars = (educationVar) => {\r\n      SelectedEducationVars.value=educationVar;\r\n      emit('update:modelValue', educationValue);\r\n    }\r\n    const mappedSelectedEducationVars = computed(() => {\r\n      return SelectedEducationVars.value?.name || SelectedEducationVars.value || 'Выбери';\r\n    })\r\n    watch(() => props.modelValue, (newVal) => {\r\n      educationValue.value = newVal\r\n    })\r\n\r\n    return { educationValue, SelectedEducationVars, toggleSelectedEducationVars,  mappedSelectedEducationVars}\r\n  }\r\n}\r\n</script>"],"mappings":"AAeA,SAASA,GAAG,EAAEC,KAAK,EAAEC,QAAQ,QAAO,KAAI;AAExC,eAAe;EACbC,KAAK,EAAE;IACLC,UAAU,EAAEC,MAAM;IAClBC,aAAa,EAAE;MACbC,IAAI,EAAEC,KAAK;MACXC,OAAOA,CAAA,EAAE;QACP,OAAO,EAAC;MACV;IACF;EACF,CAAC;EACDC,KAAKA,CAACP,KAAK,EAAE;IAACQ;EAAI,CAAC,EAAE;IACnB,MAAMC,qBAAqB,GAACZ,GAAG,CAAC,IAAI;IACpC,MAAMa,cAAa,GAAIb,GAAG,CAACG,KAAK,CAACC,UAAU;IAE3C,MAAMU,2BAA0B,GAAKC,YAAY,IAAK;MACpDH,qBAAqB,CAACI,KAAK,GAACD,YAAY;MACxCJ,IAAI,CAAC,mBAAmB,EAAEE,cAAc,CAAC;IAC3C;IACA,MAAMI,2BAA0B,GAAIf,QAAQ,CAAC,MAAM;MACjD,OAAOU,qBAAqB,CAACI,KAAK,EAAEE,IAAG,IAAKN,qBAAqB,CAACI,KAAI,IAAK,QAAQ;IACrF,CAAC;IACDf,KAAK,CAAC,MAAME,KAAK,CAACC,UAAU,EAAGe,MAAM,IAAK;MACxCN,cAAc,CAACG,KAAI,GAAIG,MAAK;IAC9B,CAAC;IAED,OAAO;MAAEN,cAAc;MAAED,qBAAqB;MAAEE,2BAA2B;MAAGG;IAA2B;EAC3G;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}