{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport CityInput from './CityInput.vue';\nimport UniversityInput from './UniversityInput.vue';\nexport default {\n  components: {\n    CityInput,\n    UniversityInput\n  },\n  data() {\n    return {\n      // Начальное состояние с одним блоком образования\n      educations: [{\n        city: '',\n        university: ''\n      }]\n    };\n  },\n  methods: {\n    // Добавить новый блок образования\n    addEducation() {\n      this.educations.push({\n        city: '',\n        university: ''\n      });\n    },\n    // Удалить блок по индексу\n    removeEducation(index) {\n      this.educations.splice(index, 1);\n    }\n  }\n};","map":{"version":3,"names":["CityInput","UniversityInput","components","data","educations","city","university","methods","addEducation","push","removeEducation","index","splice"],"sources":["D:\\Users\\danil\\untitled\\my-project\\src\\components\\EducationInput.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Образование</h2>\r\n    <!-- Перебираем список образований -->\r\n    <div v-for=\"(education, index) in educations\" :key=\"index\" class=\"education-block\">\r\n      <!-- Поле ввода города -->\r\n      <CityInput v-model=\"education.city\" />\r\n\r\n      <!-- Поле ввода университета -->\r\n      <UniversityInput v-model=\"education.university\" :city=\"education.city\" />\r\n\r\n      <!-- Кнопка для удаления блока -->\r\n      <button @click=\"removeEducation(index)\" class=\"remove-btn\">×</button>\r\n    </div>\r\n\r\n    <!-- Кнопка для добавления нового блока -->\r\n    <button @click=\"addEducation\" class=\"add-btn\">Добавить еще одно место обучения</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CityInput from './CityInput.vue';\r\nimport UniversityInput from './UniversityInput.vue';\r\n\r\nexport default {\r\n  components: { CityInput, UniversityInput },\r\n  data() {\r\n    return {\r\n      // Начальное состояние с одним блоком образования\r\n      educations: [{ city: '', university: '' }],\r\n    };\r\n  },\r\n  methods: {\r\n    // Добавить новый блок образования\r\n    addEducation() {\r\n      this.educations.push({ city: '', university: '' });\r\n    },\r\n    // Удалить блок по индексу\r\n    removeEducation(index) {\r\n      this.educations.splice(index, 1);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.education-block {\r\n  border: 1px solid #ddd;\r\n  padding: 10px;\r\n  margin-bottom: 10px;\r\n  position: relative;\r\n  border-radius: 8px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\n.remove-btn {\r\n  position: absolute;\r\n  top: 5px;\r\n  right: 5px;\r\n  background-color: #ff4d4d;\r\n  color: white;\r\n  border: none;\r\n  cursor: pointer;\r\n  border-radius: 50%;\r\n  width: 25px;\r\n  height: 25px;\r\n  font-size: 16px;\r\n  line-height: 1;\r\n}\r\n\r\n.add-btn {\r\n  background-color: #4caf50;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px 15px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  font-size: 14px;\r\n}\r\n</style>\r\n"],"mappings":";AAqBA,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,eAAc,MAAO,uBAAuB;AAEnD,eAAe;EACbC,UAAU,EAAE;IAAEF,SAAS;IAAEC;EAAgB,CAAC;EAC1CE,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,UAAU,EAAE,CAAC;QAAEC,IAAI,EAAE,EAAE;QAAEC,UAAU,EAAE;MAAG,CAAC;IAC3C,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACJ,UAAU,CAACK,IAAI,CAAC;QAAEJ,IAAI,EAAE,EAAE;QAAEC,UAAU,EAAE;MAAG,CAAC,CAAC;IACpD,CAAC;IACD;IACAI,eAAeA,CAACC,KAAK,EAAE;MACrB,IAAI,CAACP,UAAU,CAACQ,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAClC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}